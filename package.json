{
  "name": "request",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "play": "pnpm dev --filter playground",
    "clear": "rimraf 'packages/*/{node_modules}' && rimraf node_modules",
    "changeset": "changeset",
    "prettier": "prettier --write .",
    "prettier-watch": "onchange '**/*.ts' -- prettier --write {{changed}}",
    "clean": "rimraf node_modules **/*/node_modules",
    "cs:update": "changeset",
    "cs:version": "changeset version",
    "cs:tag": "changeset tag",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "prepare": "husky install",
    "commit": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "cs": "changeset && changeset version",
    "publish-packages": "turbo run build && changeset version && changeset publish"
  },
  "workspaces": [
    "playground/*",
    "packages/*"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^3.5.1",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@types/node": "^18",
    "commitizen": "^4.3.0",
    "cz-customizable": "7.0.0",
    "eslint": "^9.10.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.27.1",
    "prettier": "latest",
    "rimraf": "^4.1.2",
    "tsup": "^6.6.3",
    "turbo": "latest",
    "typescript": "^4.9.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  }
}
